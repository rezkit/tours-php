{
    "name": "rezkit/tours",
    "description": "API Client for Tour Manager",
    "type": "library",
    "require": {
        "php": ">= 7.4",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/serializer": "^5.4",
        "symfony/property-info": "^5.4",
        "doctrine/annotations": "^2.0",
        "symfony/string": "^5.4",
        "psr/cache": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/property-access": "^5.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RezKit\\Tours\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Leo Adamek",
            "email": "leo.adamek@mrzen.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        },

        "platform": {
            "php": "7.4"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpro/grumphp": "^1.13"
    }
}
